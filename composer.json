{
    "name": "sprust/sparallel",
    "description": "Parallel PHP via processes",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sprust28@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/process": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SParallel\\": "src/SParallel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SParallel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
