{
    "name": "sparallel/sparallel",
    "description": "Parallel PHP via processes",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sprust28@gmail.com"
        }
    ],
    "require": {
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-sockets": "*",
        "php": "^8.2",
        "symfony/process": "^6.4|^7.0",
        "opis/closure": "^4.3",
        "psr/container": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SParallel\\": "src/SParallel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SParallel\\Tests\\": "tests/feature"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    }
}
